{
  "post": {
    "\\s+": " ",
    "([.,!?])\\s*([.,!?])": "$1",
    "(\\w+)\\s*-\\s*(\\w+)": "$1-$2",
    "(\\w+)\\s*(\\w+)": "$1 $2",
    "(\\s*)\\\\.\\s*\\\\.\\s*\\\\.(\\s*)": "$1...$2",
    "(\\s*)\\\\!\\s*\\\\!\\s*\\\\!(\\s*)": "$1!!!$2",
    "(\\s*)\\\\?\\s*\\\\?\\s*\\\\?(\\s*)": "$1???$2",
    "(\\s*)\\\\!\\s*\\\\?(\\s*)": "$1!?$2",
    "(\\s*)\\\\?\\s*\\\\!(\\s*)": "$1?!$2",
    "(\\s*)\\\\.\\s*(\\s*)": ". ",
    "(\\s*),\\s*(\\s*)": ", ",
    "(\\s*);\\s*(\\s*)": "; ",
    "(\\s*):\\s*(\\s*)": ": ",
    "(\\s*)\\\\)\\\\s*(\\s*)": ") ",
    "(\\s*)\\((\\s*)": " (",
    "(\\s*)\\[(\\s*)": " [",
    "(\\s*)\\]\\s*(\\s*)": "] ",
    "(\\s*)\\{(\\s*)": " {",
    "(\\s*)\\}\\s*(\\s*)": "} ",
    "(\\s*)\\\"(\\s*)": " \\\"",
    "(\\s*)\\\"\\s*(\\s*)": "\\\" ",
    "(\\s*)\\'(\\s*)": " \\'",
    "(\\s*)\\'\\s*(\\s*)": "\\' "
  }
}
